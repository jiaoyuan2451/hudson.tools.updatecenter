            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <netbeans.deploy>true</netbeans.deploy>
                <netbeans.deploy.clientUrlPart>rest/artifacts?path=org/jvnet</netbeans.deploy.clientUrlPart>
                <netbeans.deploy.profilemode>true</netbeans.deploy.profilemode>
            </properties>
        </action>
